<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CampMobile" parent="Theme.MaterialComponents.DayNight.DarkActionBar" tools:ignore="ResourceName">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_default</item>
        <item name="colorPrimaryVariant">@color/color_default</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/color_default</item>
        <item name="colorSecondaryVariant">@color/color_default</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>